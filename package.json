{
  "name": "typegraphql-nestjs",
  "version": "1.0.0-alpha",
  "description": "Basic integration of TypeGraphQL in NestJS. Allows to use TypeGraphQL features while integrating with NestJS modules system and dependency injector.",
  "keywords": [
    "type-graphql",
    "nest",
    "nestjs",
    "graphql"
  ],
  "homepage": "https://github.com/kasi-labs/typegraphql-nestjs#readme",
  "bugs": {
    "url": "https://github.com/kasi-labs/typegraphql-nestjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasi-labs/typegraphql-nestjs.git"
  },
  "license": "MIT",
  "author": "MichaÅ‚ Lytek (https://github.com/MichalLytek)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "files": [
    "dist",
    "pnpm-lock.yaml",
    "tsconfig.json",
    "typegraphql-logo.png"
  ],
  "scripts": {
    "build": "tsc",
    "prepublish": "pnpm build",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "pnpm typecheck -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/federation-internals": "^2.1.4",
    "@graphql-tools/utils": "^8.12.0",
    "tslib": "^2.4.0",
    "typegraphql-nestjs": "link:"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.1.4",
    "@apollo/subgraph": "^2.1.4",
    "apollo-server-core": "^3.10.3",
    "@graphql-tools/schema": "^9.0.4",
    "@nestjs/apollo": "^10.0.5",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.3.1",
    "@nestjs/graphql": "10.0.22",
    "@nestjs/platform-fastify": "^8.0.6",
    "@nestjs/testing": "^8.0.6",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.6.2",
    "apollo-server-fastify": "^3.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "fastify": "^3.27.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.5",
    "graphql-tools": "^8.1.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.4.0",
    "ts-jest": "^27.0.5",
    "ts-morph": "^15.0.0",
    "ts-node": "^10.2.1",
    "type-graphql": "workspace:2.0.0-alpha",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.3.1",
    "@nestjs/graphql": "10.0.22",
    "graphql-tag": "^2.12.5",
    "type-graphql": "workspace:2.0.0-alpha",
    "cache-manager": "*"
  },
  "engines": {
    "node": ">= 14"
  }
}
